{
  "name": "lin3s-front-foundation",
  "version": "0.6.1",
  "author": "LIN3S",
  "license": "MIT",
  "description": "Base CSS and JS files for building views in LIN3S way",
  "keywords": [
    "lin3s",
    "front",
    "foundation",
    "es2015",
    "js",
    "scss"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "phoenix": "rimraf node_modules && yarn install",
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && babel --copy-files --out-dir dist/js src/js && cp -R src/scss dist"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "lin3s-event-bus": "^0.6.0",
    "parsleyjs": "^2.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "rimraf": "^2.5.4"
  }
}
